DEFINITION MODULE [2] fcntl;

FROM SYSTEM IMPORT int;

VAR
  O_RDONLY      -: int;
  O_WRONLY      -: int;
  O_RDWR        -: int;
  O_APPEND      -: int;
  O_CREAT       -: int;
  O_TRUNC       -: int;
  O_EXCL        -: int;
  O_TEXT        -: int;
  O_BINARY      -: int;
  O_RAW         -: int;
  O_TEMPORARY   -: int;
  O_NOINHERIT   -: int;
  O_SEQUENTIAL  -: int;
  O_RANDOM      -: int;
  S_IREAD       -: int;
  S_IWRITE      -: int;
  S_IRWXG       -: int;
  S_IRWXO       -: int;
  S_IRGRP       -: int;
  S_IWGRP       -: int;
  S_IROTH       -: int;
  S_IWOTH       -: int;

END fcntl.
