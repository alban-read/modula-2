(*
 * XDS runtime library.
 * Implementation for operations on "SET OF" operands
 * for code generated by C and X86 back-ends.
 *)
<*+ M2EXTENSIONS *>
DEFINITION MODULE ["C"] xrLSETs;

IMPORT  xmRTS;
FROM SYSTEM    IMPORT  INT16, INT32, CARD16, CARD32;

PROCEDURE X2C_ROTL(res: xmRTS.LSET; a: xmRTS.LSET; len: INT16; n: INT32): xmRTS.LSET;
PROCEDURE X2C_LSHL(res: xmRTS.LSET; a: xmRTS.LSET; len: INT16; n: INT32): xmRTS.LSET;
PROCEDURE X2C_INCL(set: xmRTS.LSET; i: CARD32; bits: CARD16): xmRTS.LSET;
PROCEDURE X2C_EXCL(set: xmRTS.LSET; i: CARD32; bits: CARD16): xmRTS.LSET;

PROCEDURE X2C_SET_EQU(a,b: xmRTS.LSET; bits: CARD16): BOOLEAN;
PROCEDURE X2C_SET_LEQ(a,b: xmRTS.LSET; bits: CARD16): BOOLEAN;

PROCEDURE X2C_LONGSET(set: xmRTS.LSET; a,b: CARD32; bits: CARD16): xmRTS.LSET;
<* IF __GEN_C__ THEN *>
PROCEDURE X2C_INL(i: CARD32; bits: CARD16; set: xmRTS.LSET): BOOLEAN;
<* END *>

PROCEDURE X2C_AND(res,a,b: xmRTS.LSET; len: CARD16): xmRTS.LSET;
PROCEDURE X2C_OR (res,a,b: xmRTS.LSET; len: CARD16): xmRTS.LSET;
PROCEDURE X2C_XOR(res,a,b: xmRTS.LSET; len: CARD16): xmRTS.LSET;
PROCEDURE X2C_BIC(res,a,b: xmRTS.LSET; len: CARD16): xmRTS.LSET;

PROCEDURE X2C_COMPLEMENT(res,a: xmRTS.LSET; len: CARD16): xmRTS.LSET;

END xrLSETs.
