<*+ M2EXTENSIONS *>
DEFINITION MODULE ["C"] xrSETs;

FROM SYSTEM    IMPORT  INT32, INT16, SET32;

PROCEDURE X2C_ASH(a: INT32; b: INT32): INT32;
PROCEDURE X2C_ROT(a: SET32; len: INT16; n: INT32): SET32;
PROCEDURE X2C_LSH(a: SET32; len: INT16; n: INT32): SET32;

END xrSETs.
