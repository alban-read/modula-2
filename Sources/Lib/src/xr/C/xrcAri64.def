(*
 * XDS runtime library.
 * 64-bit arithmetics functions for code generated by C back-end.
 *)
<*+ M2EXTENSIONS *>
DEFINITION MODULE ["C"] xrcAri64;

FROM SYSTEM IMPORT INT64;

PROCEDURE X2C_REM64_F(a: INT64; b: INT64): INT64;
PROCEDURE X2C_QUO64_F(a: INT64; b: INT64): INT64;
PROCEDURE X2C_MOD64_F(a: INT64; b: INT64): INT64;
PROCEDURE X2C_DIV64_F(a: INT64; b: INT64): INT64;

PROCEDURE X2C_ABS_INT64(x: INT64): INT64;

END xrcAri64.
