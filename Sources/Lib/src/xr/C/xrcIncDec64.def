(*
 * XDS runtime library.
 * INC() and DEC() operators for code generated by C back-end.
 *)
<*+ M2EXTENSIONS *>
DEFINITION MODULE ["C"] xrcIncDec64;

FROM SYSTEM    IMPORT  INT64,CARD64;

PROCEDURE X2C_INC64  (VAR x: INT64;  y,min,max: INT64): INT64;
PROCEDURE X2C_INCU64 (VAR x: CARD64; y,min,max: CARD64): CARD64;

PROCEDURE X2C_DEC64  (VAR x: INT64;  y,min,max: INT64): INT64;
PROCEDURE X2C_DECU64 (VAR x: CARD64; y,min,max: CARD64): CARD64;

END xrcIncDec64.
